    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <FrameLayout
            android:id="@+id/fragment_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="-50dp" />

        <Button
            android:id="@+id/car_plate_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="车牌"
            android:textSize="13sp" />

        <Button
            android:id="@+id/vin_id_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/car_plate_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="车架"
            android:textSize="13sp" />

        <Button
            android:id="@+id/id_card_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/vin_id_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="身份证"
            android:textSize="13sp" />

        <Button
            android:id="@+id/vehicle_id_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/id_card_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="模型测试"
            android:textSize="13sp" />

        <Button
            android:id="@+id/driver_id_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/vehicle_id_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="驾驶证"
            android:textSize="13sp" />

        <Button
            android:id="@+id/car_front_45_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/driver_id_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="前45°"
            android:textSize="13sp" />

        <Button
            android:id="@+id/car_back_45_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/car_front_45_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="后45°"
            android:textSize="13sp" />

        <Button
            android:id="@+id/car_damage_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/car_back_45_button"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="-5dp"
            android:text="损伤部位"
            android:textSize="13sp" />
    </RelativeLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>